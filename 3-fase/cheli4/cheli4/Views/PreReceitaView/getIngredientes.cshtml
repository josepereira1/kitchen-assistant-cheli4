@model IEnumerable<cheli4.Models.Comercial.Ingrediente>
@{
    ViewData["Title"] = "getIngredientes";
}

<h1>getIngredientes</h1>

<table class="table">
    <tr>
        <td>Nome</td>
    </tr>
    @foreach (var receita in Model)
    {
        <tr>
            <td>@receita.id - @receita.nome</td>
        </tr>
    }
</table>
