@model IEnumerable<cheli4.Models.Comercial.ReceitaIngrediente>
@{
    ViewData["Title"] = "getReceitasIngredientes";
}

<h1>getReceitasIngredientes</h1>

<table class="table">
    <tr>
        <td>Nome</td>
    </tr>
    @foreach (var receita in Model)
    {
        <tr>
            <td>@receita.FK_id_ingrediente - @receita.FK_id_receita - @receita.quantidade</td>
        </tr>
    }
</table>
